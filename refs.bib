  title = {“{{As}} Long as That Is My Hand, That Willed Action Is Mine”: {{Timing}} of Agency Triggered by Body Ownership},
  shorttitle = {“{{As}} Long as That Is My Hand, That Willed Action Is Mine”},
  author = {Burin, Dalila and Pyasik, Maria and Ronga, Irene and Cavallo, Marco and Salatino, Adriana and Pia, Lorenzo},
