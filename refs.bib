@article{schurgerAccumulatorModelSpontaneous2012,
  title = {An Accumulator Model for Spontaneous Neural Activity Prior to Self-Initiated Movement},
  author = {Schurger, Aaron and Sitt, Jacobo D. and Dehaene, Stanislas},
  date = {2012-10-16},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {109},
  number = {42},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1210467109},
  url = {https://pnas.org/doi/full/10.1073/pnas.1210467109},
  urldate = {2023-07-25},
  abstract = {A gradual buildup of neuronal activity known as the “readiness potential” reliably precedes voluntary self-initiated movements, in the average time locked to movement onset. This buildup is presumed to reflect the final stages of planning and preparation for movement. Here we present a different interpretation of the premovement buildup. We used a leaky stochastic accumulator to model the neural decision of “when” to move in a task where there is no specific temporal cue, but only a general imperative to produce a movement after an unspecified delay on the order of several seconds. According to our model, when the imperative to produce a movement is weak, the precise moment at which the decision threshold is crossed leading to movement is largely determined by spontaneous subthreshold fluctuations in neuronal activity. Time locking to movement onset ensures that these fluctuations appear in the average as a gradual exponential-looking increase in neuronal activity. Our model accounts for the behavioral and electroencephalography data recorded from human subjects performing the task and also makes a specific prediction that we confirmed in a second electroencephalography experiment: Fast responses to temporally unpredictable interruptions should be preceded by a slow negative-going voltage deflection beginning well before the interruption itself, even when the subject was not preparing to move at that particular moment.           ,              Author Summary                            The role of ongoing spontaneous fluctuations in neural activity has been widely studied in the domain of perceptual decision making, but their potential role in self-initiated movement has been largely overlooked. The model that we have introduced here unifies perceptual decision making and spontaneous or “voluntary” movement generation. It may generalize to other contexts where unpredictable spontaneous behavior is called for, such as in certain sports, musical improvisation, or game theory. Our work may also inform the development of asynchronous brain–computer interfaces, because spontaneous fluctuations in motor-related activity may be a source of false positives when trying to detect a user’s intentions in real time. Most importantly, our work may help to resolve “Libet’s paradox” (               3               ): Why should there be such a long and highly variable gap between the time of a motor decision and the subjective estimate of the time of the motor decision? In fact, this question arises only when we assume that the motor decision coincides in time with the onset of the RP. We have argued that this need not be the case and that the neural decision to move may come much closer in time to the movement itself, coincident with average subjective estimates of the time of the intention to move (               3               ).                                         One limitation of our model is that it treats each trial as completely independent of the one before it, although empirically we observed a clear relationship (positive correlation) between the waiting times on trials               t               and               t               − 1. The model is also silent with regard to a variety of other factors that might contribute to determining the waiting time on each trial. For example, subjects may strategically try to produce a “random” distribution of waiting times across trials. These factors could be incorporated into a future model, but it is important to note that the model was able to account for our data with only three free parameters.                          Simulations showed that when the model is “interrupted” at random times and forced to produce a speeded response, the fastest responses are preceded by a gradual “buildup” (in the direction of the threshold) that long precedes the interruption itself, whereas the slower responses are not. Remarkably, we found the same to be true of our empirical data. Of course, it could be that fast responses were fast because the subject just happened to be preparing a spontaneous movement when the click occurred. We anticipated this possible interpretation and so asked subjects to say the word “coincidence” if they ever felt that they were about to make a movement at the time of the click, and these trials, although rare, were excluded from the analysis. Other interpretations could also be ruled out. Thus, the RP, which appears to be an intentional goal-directed buildup, might instead reflect ongoing random fluctuations in neural activity.                            In addition, our model also directed us to a specific prediction that we tested with a second EEG experiment. Implicit in our model is the notion that the motor system constantly undergoes covert fluctuations that bring it closer to or farther from threshold. We reasoned that we should be able to capture these ongoing fluctuations by randomly interrupting subjects with a compulsory response cue and sorting trials by their reaction times. We used a variant of Libet’s task that we refer to as               Libetus interruptus               : Subjects perform the standard Libet task, but are told that they may sometimes be interrupted by an auditory “click,” in which case they are to perform the movement (a button press) immediately and as quickly as possible (                                Fig. P1,                 Bottom                              ). Assuming that the interrupted responses arise from the same decision accumulator as the self-initiated ones, short response times should come primarily from trials in which the spontaneous fluctuations were already close to threshold. Hence, fast sensory-cued responses should also be preceded by a readiness potential.                                         Our experimental results show that a decision threshold applied to autocorrelated noise—in this case the output of a leaky stochastic accumulator—can account for the specific shape of the RP as well as the distribution of waiting times from subjects performing Libet et al.’s (               3               ) spontaneous-movement task. We replicated Libet’s (1983) behavioral and electroencephalography (EEG) results and validated our model by fitting the shape of the RP, using parameters chosen by fitting the distribution of threshold-crossing times in the model to the empirical distribution of waiting times.                                         According to the model, when the imperative to produce a movement is weak and nonspecific (as in Libet’s task), the precise moment at which the decision threshold is crossed leading to movement is largely determined by spontaneous subthreshold fluctuations (               Fig. P1               ). Because these fluctuations exhibit temporal autocorrelation, time locking to movement onset will ensure that a gradual exponential-looking buildup appears in the average. Thus, two assumptions are required by the model to produce a readiness potential without any immediate intention to move: (               i               ) Ongoing fluctuations in neural activity exhibit temporal autocorrelation, and (               ii               ) ongoing fluctuations play a role in determining the precise time of movement onset, to which the data epochs are time locked. The former is well known to be the case from prior research (               5               ), and the latter is taken up in the present study.                          One solution, given these instructions, is to allow intrinsic noise to accumulate in the decision system. Our model for spontaneous movement supposes that subjects shift motor activation up closer to the threshold for initiation of the instructed movement, integrate intrinsic noise, and wait for a random threshold-crossing event. We refer to this as the stochastic-decision model and implement it using a leaky stochastic accumulator process with three parameters.                            We propose that, to comply with the instruction to produce spontaneous movements, the brain uses the same mechanism as for any decision-making task: a threshold applied to the output of a neural accumulator (               4               ). Decision-making tasks are typically modeled in terms of the accumulation of sensory evidence. What is unique to the spontaneous-movement task is that subjects are specifically instructed to not base their movement decisions on sensory evidence. Subjects are also instructed to not use strategies such as counting (and intentionally varying) time elapsed.                                         In Libet et al.’s (               3               ) spontaneous-movement task subjects are asked to make a single “capricious” movement on each trial, without deciding in advance               when               the movement will be made—the movement should be maximally “spontaneous.” We refer to the time between the start of the trial and the execution of the movement as the “waiting time.” The questions we pose (and to which we propose an answer) are as follows: (               i               ) Why is there a gradual buildup of neuronal activity preceding spontaneous self-initiated movements? And (               ii               ) how might this relate to the way the brain generates spontaneous movements in a task like Libet’s?                                         The term               bereitschaftspotential               or “readiness potential” (RP) refers to a slow 1- to 2-s buildup of scalp electrical potential that reliably precedes self-initiated movements, in the average over many data epochs time locked to movement onset. Discovered in 1965 by Kornhuber and Deecke (               1               ), the RP has since been linked to changes in neuronal firing rates in the supplementary motor area (               2               ) and is widely assumed to reflect an intentional sequence of neural operations directed at producing a movement (               1               ). This assumption has become a source of controversy because human subjects appear to be unaware of their intention until only ∼200 ms before the onset of the movement (               3               ).},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/Information seeking/new/pnas.1210467109.pdf}
}

@article{schurmannHowCognitiveModels2019,
  title = {How {{Cognitive Models}} of {{Human Body Experience Might Push Robotics}}},
  author = {Schürmann, Tim and Mohler, Betty Jo and Peters, Jan and Beckerle, Philipp},
  date = {2019-04-11},
  journaltitle = {Frontiers in Neurorobotics},
  shortjournal = {Front. Neurorobot.},
  volume = {13},
  pages = {14},
  issn = {1662-5218},
  doi = {10.3389/fnbot.2019.00014},
  url = {https://www.frontiersin.org/article/10.3389/fnbot.2019.00014/full},
  urldate = {2023-06-28},
  abstract = {In the last decades, cognitive models of multisensory integration in human beings have been developed and applied to model human body experience. Recent research indicates that Bayesian and connectionist models might push developments in various branches of robotics: assistive robotic devices might adapt to their human users aiming at increased device embodiment, e.g., in prosthetics, and humanoid robots could be endowed with human-like capabilities regarding their surrounding space, e.g., by keeping safe or socially appropriate distances to other agents. In this perspective paper, we review cognitive models that aim to approximate the process of human sensorimotor behavior generation, discuss their challenges and potentials in robotics, and give an overview of existing approaches. While model accuracy is still subject to improvement, human-inspired cognitive models support the understanding of how the modulating factors of human body experience are blended. Implementing the resulting insights in adaptive and learning control algorithms could help to taylor assistive devices to their user’s individual body experience. Humanoid robots who develop their own body schema could consider this body knowledge in control and learn to optimize their physical interaction with humans and their environment. Cognitive body experience models should be improved in accuracy and online capabilities to achieve these ambitious goals, which would foster human-centered directions in various fields of robotics.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/active self/reviews/fnbot-13-00014.pdf}
}

@article{schwarzPreciseFeatureGaina,
  title = {Precise Feature Gain Tuning in Visual Search Tasks - the Basal Ganglia Makes It Possible: {{A}} Neuro-Computational Study},
  author = {Schwarz, Alex and Hamker, Dr Fred},
  abstract = {A common theory is that when looking for a known target, the visual attention is directed to the features of the target, thus enhancing the response to the target in the visual system. In a study by Navalpakkam \& Itti (2007) it was shown that subjects in a visual search task, in which the target was very similar to the distractors, did not learn to direct the attention directly to the features of the target, as this would also enhance the response to the distractors. In this work, the visual search task of Navalpakkam \& Itti (2007) was performed with a biologically plausible neural model. For this a model of the visual system (Beuth, 2017) was combined with a model of the basal ganglia (Villagrasa, Baladron, \& Hamker, 2016). In this combined model, the basal ganglia receive information about the visual input through the inferior temporal cortex (IT) and control the activity of the prefrontal cortex (PFC). Activity in the PFC causes feature-based attention in the visual model. The combined model can select certain items of the visual input through feature-based and spatial attention and learns to direct the feature-based attention to certain features due to reward. The findings of Navalpakkam \& Itti (2007) could be replicated with the model. The model thus demonstrates a very interesting, so far poorly investigated function of the basal ganglia, which could also explain other findings about the effects of reward on attention.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/attention/MA_OliverMaith.pdf}
}

