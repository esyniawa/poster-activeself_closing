@article{livingstoneSignalEnhancementNot2017,
  title = {Signal Enhancement, Not Active Suppression, Follows the Contingent Capture of Visual Attention.},
  author = {Livingstone, Ashley C. and Christie, Gregory J. and Wright, Richard D. and McDonald, John J.},
  date = {2017},
  journaltitle = {Journal of Experimental Psychology: Human Perception and Performance},
  shortjournal = {Journal of Experimental Psychology: Human Perception and Performance},
  volume = {43},
  number = {2},
  pages = {219--224},
  issn = {1939-1277, 0096-1523},
  doi = {10.1037/xhp0000339},
  url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/xhp0000339},
  urldate = {2023-05-05},
  abstract = {When searching for a specific visual object, such as a red jacket, attention can be drawn to irrelevant items that possess the key feature, such as a red scarf. In this example, how does attending to the red scarf (a distractor) affect your ability to later find your jacket (the target)? One hypothesis is that processing at the location of the scarf will temporarily remain enhanced, but recent electrophysiological evidence suggests that processing at that location is rapidly suppressed. Here, we show that reflexively orienting attention to a distractor boosts perceptual processing of subsequent items at that location and that an electrophysiological component thought to reflect distractor suppression reflects instead the enhancement of a subsequent item appearing at the same location. Enhancing the perceptual representation of an irrelevant object impairs search for the target, while enhancing the perceptual representation of the target itself facilitates search.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/attention/2017-03293-001.pdf}
}

