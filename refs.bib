  author = {Milardi, Demetrio and Quartarone, Angelo and Bramanti, Alessia and Anastasi, Giuseppe and Bertino, Salvatore and Basile, Gianpaolo Antonio and Buonasera, Piero and Pilone, Giorgia and Celeste, Giuseppe and Rizzo, Giuseppina and Bruschetta, Daniele and Cacciola, Alberto},
