@article{shergillModulationSomatosensoryProcessing2013,
  title = {Modulation of Somatosensory Processing by Action},
  author = {Shergill, Sukhwinder S. and White, Thomas P. and Joyce, Daniel W. and Bays, Paul M. and Wolpert, Daniel M. and Frith, Chris D.},
