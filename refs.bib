  author = {family=Boer, given=Lieke, prefix=de, useprefix=true and Axelsson, Jan and Riklund, Katrine and Nyberg, Lars and Dayan, Peter and Bäckman, Lars and Guitart-Masip, Marc},
  date = {2017-09-05},
  journaltitle = {eLife},
