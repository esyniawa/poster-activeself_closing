@article{hooksTopographicPrecisionSensory2018,
  title = {Topographic Precision in Sensory and Motor Corticostriatal Projections Varies across Cell Type and Cortical Area},
  author = {Hooks, Bryan M. and Papale, Andrew E. and Paletzki, Ronald F. and Feroze, Muhammad W. and Eastwood, Brian S. and Couey, Jonathan J. and Winnubst, Johan and Chandrashekar, Jayaram and Gerfen, Charles R.},
