@article{yuAttentionalTemplateShifted2019,
  title = {The Attentional Template Is Shifted and Asymmetrically Sharpened by Distractor Context.},
  author = {Yu, Xinger and Geng, Joy J.},
  date = {2019-03},
  journaltitle = {Journal of Experimental Psychology: Human Perception and Performance},
  shortjournal = {Journal of Experimental Psychology: Human Perception and Performance},
  volume = {45},
  number = {3},
  pages = {336--353},
  issn = {1939-1277, 0096-1523},
  doi = {10.1037/xhp0000609},
  url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/xhp0000609},
  urldate = {2023-02-09},
  abstract = {Theories of attention hypothesize the existence of an “attentional template” in memory that contains goal-relevant information. For example, when searching for an apple, a person might hold its color (red) and shape (round) in memory while looking for it. It is frequently assumed that visual search is most efficient when one holds veridical features that perfectly match the target. However, this is not always true when the distractor context is predictable. Our work demonstrates that expectations about the distractor features and similarity to the target can cause the remembered features of the target to be exaggerated away. The results suggest that visual search is most efficient when one searches for target characteristics that are more distinct from visual distractors.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/attention/Research on the attentional template/GENG The Attentional Template Is Shifted and Asymmetrically Sharpened by.pdf}
}

