@article{brownLearnedPredictionsError2005,
  title = {Learned {{Predictions}} of {{Error Likelihood}} in the {{Anterior Cingulate Cortex}}},
  author = {Brown, Joshua W. and Braver, Todd S.},
  date = {2005-02-18},
  journaltitle = {Science},
  shortjournal = {Science},
  volume = {307},
