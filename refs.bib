@article{pattersonColorVisionCircuit2020,
  title = {A {{Color Vision Circuit}} for {{Non-Image-Forming Vision}} in the {{Primate Retina}}},
  author = {Patterson, Sara S. and Kuchenbecker, James A. and Anderson, James R. and Neitz, Maureen and Neitz, Jay},
  date = {2020-04},
  journaltitle = {Current Biology},
  shortjournal = {Current Biology},
