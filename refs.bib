@article{desmurgetMotorSequencesBasal2010,
  title = {Motor {{Sequences}} and the {{Basal Ganglia}}: {{Kinematics}}, {{Not Habits}}},
  shorttitle = {Motor {{Sequences}} and the {{Basal Ganglia}}},
  author = {Desmurget, Michel and Turner, Robert S.},
