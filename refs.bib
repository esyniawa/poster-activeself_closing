}

@article{salinasBookReviewGain2001,
  title = {Book {{Review}}: {{Gain Modulation}} in the {{Central Nervous System}}: {{Where Behavior}}, {{Neurophysiology}}, and {{Computation Meet}}},
  shorttitle = {Book {{Review}}},
  author = {Salinas, Emilio and Sejnowski, Terrence J.},
  date = {2001-10},
  journaltitle = {The Neuroscientist},
  shortjournal = {Neuroscientist},
  volume = {7},
