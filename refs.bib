@article{smithThalamostriatalSystemNormal2014,
  title = {The Thalamostriatal System in Normal and Diseased States},
  author = {Smith, Yoland and Galvan, Adriana and Ellender, Tommas J. and Doig, Natalie and Villalba, Rosa M. and Huerta-Ocampo, Icnelia and Wichmann, Thomas and Bolam, J. Paul},
  date = {2014},
  journaltitle = {Frontiers in Systems Neuroscience},
